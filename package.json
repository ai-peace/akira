{
  "name": "agent-rare",
  "engines": {
    "node": ">=18.17.0"
  },
  "type": "module",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "migrate:generate": "prisma migrate dev --create-only --name",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:deploy:test": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:push:test": "prisma db push",
    "db:seed": "ts-node ./spec/seed/index.seed.ts",
    "db:reset": "prisma migrate reset --preview-feature",
    "db:reset:test": "prisma migrate reset --preview-feature",
    "codegen:llm-server": "openapi-generator-cli generate -g typescript-axios -i http://localhost:8000/api/openapi.json  -o ./server/server-api-client/generated/api -c ./openapiConfig.yml  --skip-validate-spec "
  },
